@model IEnumerable<ApplicationDevelopment.Models.Department>
@{
    ViewBag.Title = "Course";
}

<h2>All Department</h2>


<table class="table">
    <thead>
        <tr>
            <th scope="col">Department Id</th>
            <th scope="col">Department Name</th>
            <th scope="col">Action </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>@item.Id</th>
                <th>@item.DepartmentName</th>
                <td>
                    @Html.ActionLink("StaffViewDepartmentAll", "StaffViewDepartmentAll", new { Id = item.Id }, new { @class = "btn btn-success" })
                </td>
            </tr>
        }
    </tbody>
</table>








